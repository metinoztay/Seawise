    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/themedata/adminlte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Bootstrap Switch -->
    <script src="~/themedata/adminlte/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <!-- BS-Stepper -->
    <script src="~/themedata/adminlte/plugins/bs-stepper/js/bs-stepper.min.js"></script>
    <!-- dropzonejs -->
    <script src="~/themedata/adminlte/plugins/dropzone/min/dropzone.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/themedata/adminlte/dist/js/adminlte.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <script>
        var year = new Date().getFullYear();
        if (year != 2024)
            document.getElementById("currentYear").textContent = " - " + year;
    </script>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var activeTabId = '@ViewBag.ActiveTabId'; // ViewBag'den gelen aktif tab ID'si
            var activeLink = document.getElementById(activeTabId); // İlgili linki bul

            if (activeLink) {
                // Aktif linke 'active' sınıfını ekle
                activeLink.classList.add('active');
            }
        });
    </script>

</body>
</html>